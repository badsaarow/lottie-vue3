{
  "name": "lottie-vue3",
  "version": "0.1.0",
  "description": "Lottie Component for Vue3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "build": "vue-tsc --noEmit && vite build",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts,.vue",
    "lint:fix": "eslint src --fix --ext .js,.ts,.vue",
    "test": "jest --config jest.config.js",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/badsaarow/lottie-vue3.git"
  },
  "keywords": [
    "lottie",
    "vue3"
  ],
  "author": "Sungyong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/badsaarow/lottie-vue3/issues"
  },
  "homepage": "https://github.com/badsaarow/lottie-vue3#README.md",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.16.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.2",
    "vite": "^2.9.16",
    "vue-tsc": "^0.2.2"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "lottie-web": "^5.7.13",
    "vue": "^3.2.6"
  }
}
